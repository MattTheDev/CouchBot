{
  "Name": "CouchBot",
  "Version": 3.0,
  "Prefix": "!cb3",
  "Keys": {
    /*
    
    Discord Bot Token is created here: https://discordapp.com/developers/applications/
    Twitch Client Id is created here: https://glass.twitch.tv/console/apps/create
    Mixer Client Id is created here: https://mixer.com/lab/oauth
    YouTube Api Key is created here: https://console.developers.google.com/
    
    */
    "DiscordBotToken": "",
    "TwitchClientId": "",
    "MixerClientId": "",
    "YoutubeApiKey": ""
  },
  /*
   
    Your Owner Id is found in the Discord client by enabling developer mode, right clicking your name, and clicking copy ID.
    The Bot Id is found here, https://discordapp.com/developers/applications/, under "Client Id". Or you can right click the bot user, and copy ID as well.
   
   */
  "Ids": {
    "OwnerId": "",
    "BotId": ""
  },
  /*
   
    Set the path where you want all your stuff to be saved.
   
   */
  "Directories": {
    "BotDataRoot": "C:\\ProgramData\\CouchBot3\\",
    "GuildConfigurations": "Guilds\\",
    "LiveLocks": "Live\\",
    "Logs": "Logs\\"
  },
  /*
   
   Set the seconds between checks for live content. Please be VERY careful here...
   
   */
  "Timers": {
    "TwitchTimer": 60,
    "TwitchDiscoveryTimer": 30,
    "TwitchTeamsTimer": 60
  },
  "ConnectionStrings": {
    "CouchBot": ""
  },
  "Localization": {
    "TranslationFiles": "https://github.com/MattTheDev/CouchBot-Localization/archive/master.zip"
  }
}